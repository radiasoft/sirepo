&run_setup
  lattice = "test1.lte",
  p_central_mev = 1001,
  use_beamline = "BL1",
&end

&run_control
&end

&bunched_beam
  alpha_x = -1.0,
  alpha_y = -0.0,
  alpha_z = 0.1,
  beta_x = 1.1,
  beta_y = 0.7,
  beta_z = 0.7,
  distribution_cutoff[0] = 3, 3, 3,
  emit_x = 1.4e-06,
  emit_y = 1.5e-06,
  emit_z = "(1.8e-06 sr_beta sr_gamma * /)",
  enforce_rms_values[0] = 1, 1, 1,
  n_particles_per_bunch = 5000,
&end

&track
&end