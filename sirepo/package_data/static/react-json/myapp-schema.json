{
    "type": {
        "Gender": {
            "base": "Enum",
            "config": {
                "allowedValues": [
                    ["male", "Male"],
                    ["female", "Female"]
                ]
            }
        },
        "DogDisposition": {
            "base": "Enum",
            "config": {
                "allowedValues": [
                    ["aggressive", "Aggressive"],
                    ["friendly", "Friendly"],
                    ["submissive", "Submissive"]
                ]
            }
        }
    },
    "model": {
        "dog": {
            "breed": {
                "displayName": "Breed",
                "type": "String"
            },
            "gender": {
                "displayName": "Gender",
                "type": "Gender",
                "defaultValue": "male"
            },
            "height": {
                "displayName": "Height [cm]",
                "type": "Float",
                "defaultValue": 50,
                "description": "Distance from front paws to withers"
            },
            "weight": {
                "displayName": "Weight [lbs]",
                "type": "Float",
                "defaultValue": 60.5
            },
            "disposition": {
                "displayName": "Disposition",
                "type": "DogDisposition",
                "defaultValue": "friendly"
            },
            "favoriteTreat": {
                "displayName": "Favorite Treat",
                "type": "OptionalString",
                "defaultValue": ""
            }
        }
    },
    "view": [
        {
            "layout": "navbarModalButton",
            "config": {
                "name": "simulationRenameNavButton",
                "title": "$(simulation.name)",
                "icon": "faPencil",
                "modal": {
                    "title": "Simulation",
                    "items": [
                        {
                            "layout": "fieldList",
                            "config": { "name": "simulation", "fields": ["simulation.name"] }
                        }
                    ]
                }
            }
        },
        {
            "layout": "navTabs",
            "config": {
                "name": "navtabs",
                "tabs": [
                    {
                        "name": "source",
                        "title": "Source",
                        "icon": "",
                        "items": [
                            {
                                "layout": "panel",
                                "config": {
                                    "title": "Dog",
                                    "basic": [
                                        {
                                            "layout": "tabs",
                                            "config": {
                                                "tabs": [
                                                    {
                                                        "name": "Physical Characteristics",
                                                        "items": [
                                                            {
                                                                "layout": "fieldTable",
                                                                "config": {
                                                                    "columns": ["Height", "Weight"],
                                                                    "rows": [
                                                                        {
                                                                            "label": "Size/Shape",
                                                                            "fields": [
                                                                                "dog.height",
                                                                                "dog.weight"
                                                                            ]
                                                                        }
                                                                    ]
                                                                }
                                                            },
                                                            {
                                                                "layout": "fieldList",
                                                                "config": {
                                                                    "fields": ["dog.breed"]
                                                                }
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "name": "Personality",
                                                        "items": [
                                                            {
                                                                "layout": "fieldList",
                                                                "config": {
                                                                    "fields": [
                                                                        "dog.disposition",
                                                                        "dog.favoriteTreat"
                                                                    ]
                                                                }
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                        }
                                    ],
                                    "advanced": [
                                        {
                                            "layout": "fieldList",
                                            "config": {
                                                "fields": [
                                                    "dog.breed",
                                                    "dog.gender",
                                                    "dog.weight",
                                                    "dog.height"
                                                ]
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "layout": "panel",
                                "config": {
                                    "title": "Physical Characteristics",
                                    "basic": [
                                        {
                                            "layout": "autoRunReport",
                                            "config": {
                                                "report": "heightWeightReport",
                                                "dependencies": [
                                                    "dog.*"
                                                ],
                                                "reportLayout": {
                                                    "layout": "graph2d",
                                                    "config": {}
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                ]
            }
        }
    ]
}
