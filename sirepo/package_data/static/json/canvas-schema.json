{
    "appModes": {
        "default": {
            "localRoute": "lattice"
        }
    },
    "constants": {
        "particleMassAndCharge": {
            "positron": [0.510998950e-03, 1],
            "electron": [0.510998950e-03, -1],
            "proton": [0.93827208816, 1]
        }
    },
    "dynamicFiles": {
        "externalLibs": {
            "js": [
                "split-1.3.5.min.js"
            ]
        },
        "sirepoLibs": {
            "js": [
                "canvas.js",
                "sirepo-command.js",
                "sirepo-lattice.js",
                "impactx-util.js"
            ],
             "css": [
                 "lattice.css",
                 "command.css"
            ]
        }
    },
    "enum": {
        "ApertureShape": [
            ["elliptical", "elliptical"],
            ["rectangular", "rectangular"]
        ],
        "Distribution": [
            ["File", "From File"],
            ["Gaussian", "Gaussian"],
            ["KVdist", "KVdist"],
            ["Kurth4D", "Kurth4D"],
            ["Kurth6D", "Kurth6D"],
            ["Semigaussian", "Semigaussian"],
            ["Thermal", "Thermal"],
            ["Triangle", "Triangle"],
            ["Waterbag", "Waterbag"]
        ],
        "MultipoleIndex": [
            ["1", "dipole"],
            ["2", "quadrupole"],
            ["3", "sextupole"],
            ["4", "octupole"]
        ],
        "PhaseSpaceCoordinate": [
            ["x", "x"],
            ["px", "Px"],
            ["y", "y"],
            ["py", "Py"],
            ["t", "t"],
            ["pt", "Pt"],
            ["qm", "q"]
        ],
        "Species": [
            ["positron", "Positron"],
            ["electron", "Electron"],
            ["proton", "Proton"]
        ]
    },
    "frameIdFields": {
        "sigmaAnimation": [
        ],
        "twissAnimation": [
        ],
        "bunchAnimation1": [
            "simCode",
            "histogramBins"
        ],
        "bunchAnimation2": [
            "simCode",
            "histogramBins"
        ],
        "bunchAnimation3": [
            "simCode",
            "histogramBins"
        ]
    },
    "localRoutes": {
        "lattice": {
            "route": "/lattice/:simulationId",
            "config": {
                "controller": "LatticeController as lattice",
                "template": "<div data-lattice-tab=\"\" data-controller=\"lattice\"></div>"
            }
        },
        "source": {
            "config": {
                "controller": "SourceController as source",
                "template": "<div data-impactx-source-tab=\"\" data-source=\"source\"></div>"
            }
        },
        "comparison": {
            "route": "/comparison/:simulationId",
            "config": {
              "controller": "ComparisonController as comparison",
              "templateUrl": "/static/html/canvas-comparison.html"
            }
        }
    },
    "model": {
        "beamline": {
            "name": ["Name", "ValidatedString"]
        },
        "bunchAnimation1": {
            "simCode": ["", "String", "elegant"],
            "histogramBins": ["Histogram Bins", "Integer", 80],
            "notes": ["Notes", "Text", ""]
        },
        "bunchAnimation2": {
            "simCode": ["", "String", "madx"],
            "histogramBins": ["Histogram Bins", "Integer", 80],
            "notes": ["Notes", "Text", ""]
        },
        "bunchAnimation3": {
            "simCode": ["", "String", "impactx"],
            "histogramBins": ["Histogram Bins", "Integer", 80],
            "notes": ["Notes", "Text", ""]
        },
        "bunchReport": {
            "x": ["Phase Space Coordinate", "PhaseSpaceCoordinate", "x"],
            "y": ["Phase Space Coordinate", "PhaseSpaceCoordinate", "px"],
            "histogramBins": ["Histogram Bins", "Integer", 80],
            "colorMap": ["Color Map", "ColorMap", "viridis"],
            "notes": ["Notes", "Text", ""]
        },
        "distribution": {
            "species": ["Species", "Species", "electron"],
            "energy": ["Energy [GeV]", "RPNValue", 1.0],
