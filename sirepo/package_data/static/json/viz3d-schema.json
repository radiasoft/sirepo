{
    "appModes": {
        "default": {
            "localRoute": "visualization"
        }
    },
    "constants": {
         "canDownloadInputFile": false,
         "canExportArchive": false,
         "canShowDocumentationUrl": false
    },
    "dynamicFiles": {
        "sirepoLibs": {
            "js": ["viz3d.js"]
        }
    },
    "localRoutes": {
        "visualization": {
            "route": "/visualization/:simulationId",
            "config": {
                "controller": "Viz3dVisualizationController as visualization",
                "templateUrl": "/static/html/viz3d-visualization.html"
            }
        }
    },
    "model": {
        "simulationStatus": {},
        "viz3dReport": {}
    },
    "view": {
        "simulationStatus": {
            "title": "Simulation Status",
            "advanced": []
        }
    }
}
