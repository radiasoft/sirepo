                "connectToServer"
            ],
            "advanced": []
        },
        "KICKER": {
            "basic": [
                "hkick",
                "vkick"
            ],
            "advanced": []
        },
        "QUAD": {
            "basic": [
                "k1"
            ],
            "advanced": []
        },
        "WATCH": {
        },
        "watchpointReport": {
        }
    }
