            ]
        },
        "beamSteering": {
            "title": "Beam Steering",
            "basic": [
                "steeringMethod",
                "useSteering"
            ],
            "advanced": []
        },
        "bunch": {
            "title": "Beam Settings",
            "basic": [
                "p_central_mev",
